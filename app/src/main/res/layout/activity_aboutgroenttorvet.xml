<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/signOut"
        android:layout_width="105dp"
        android:layout_height="56dp"
        android:layout_marginBottom="68dp"
        android:backgroundTint="@color/Peach"
        android:text="Sign out"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textView" />

    <TextView
android:id="@+id/textView"
android:layout_width="0dp"
android:layout_height="wrap_content"
android:layout_marginStart="16dp"
android:layout_marginLeft="16dp"
android:layout_marginTop="144dp"
android:layout_marginEnd="16dp"
android:layout_marginRight="16dp"
android:gravity="center_horizontal"
android:text="About Grønttrovet at Frederikstorv"
android:textAlignment="center"
android:textColor="@color/black"
android:textSize="20sp"
app:layout_constraintEnd_toEndOf="parent"
app:layout_constraintHorizontal_bias="0.0"
app:layout_constraintStart_toStartOf="parent"
app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="294dp"
        android:layout_height="220dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/vegetables" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="293dp"
        android:layout_height="80dp"
        android:layout_marginTop="8dp"
        android:text="På Grønttorvet kan du købe danske og udenlandske friske kvalitetsvarer som f.eks. blomster, ost, frugt og grønt, fisk, kød.
Oplev den hyggelige torvestemning, duft og smag."
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="294dp"
        android:layout_height="171dp"
        android:layout_marginTop="20dp"
        android:text="Følgende torvehandlere findes på Frederikstorv: Annette: frugt og grønt, slagter, Ostemanden Fra Vindblæs,
De Glade Gartnere: frugt og grønt, Fiskemanden Fra Hals, Honning v. Jørn Axelsen.

Hver onsdag og lørdag morgen året rundt rykker torveholderne ind på Frederikstorv, kl 8-14."
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />


</androidx.constraintlayout.widget.ConstraintLayout>



